<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.powerlong.sharera.dao.KeyPairMapper">

	<select id="queryServerKey" resultType="java.lang.String">
		select  SECRET_KEY FROM  table_key_pair where  SECRET_ID=#{serverid}
	</select>


</mapper>
