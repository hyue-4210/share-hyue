<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.powerlong.sharera.dao.pointForCouponMapper">

	<insert id="save" parameterType="com.powerlong.sharera.vo.pointForCouponVo">
		INSERT INTO `pointForCoupon` (`id`, `createTime`, `uid`, `projectId`, `type`, `payPoint`, `exchangeMoney`, `exchangeNum`)
		VALUES (#{id},now(),#{uid},#{projectId},#{type},#{payPoint},#{exchangeMoney},#{exchangeNum})

	</insert>
</mapper>
